{
  "compilerOptions": {
      "target": "es2021",
      "module": "ES2022",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      // "declaration": true,
      // "sourceMap": true,
      "removeComments": true,
      "outDir": "dist",
      "baseUrl": "src",
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noFallthroughCasesInSwitch": true,
      "noImplicitAny": false
  },
}
